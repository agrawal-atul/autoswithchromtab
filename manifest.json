{"manifest_version": 3, "name": "AutoSwitchTab", "description": "Swith tab in default time", "version": "1.0", "author": "Atul Agrawal",
"background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "autoswitch.png"
  },
  "options_page": "options.html",
"browser_action": {   "default_title": "Have a good day" },
"chrome_url_overrides" : {  "newtab": "newtab.html"},
 "permissions": ["tabs","activeTab","storage"]}
